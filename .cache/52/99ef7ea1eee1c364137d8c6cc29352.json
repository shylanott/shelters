+ marginTop + marginBottom);\n\t\t\t\treturn Chart;\n\t\t\t},\n\t\t\t// update scales and render chart\n\t\t\trender() {\n\t\t\t\t$containerMini.selectAll('.dog')\n\t\t\t\t\t.data(data.values)\n\t\t\t\t\t.enter()\n\t\t\t\t\t.append('div')\n\t\t\t\t\t.attr('class', 'dog')\n\n\t\t\t\treturn Chart;\n\t\t\t},\n\t\t\t// get / set data\n\t\t\tdata(val) {\n\t\t\t\tif (!arguments.length) return data;\n\t\t\t\tdata = val;\n\t\t\t\t$sel.datum(data);\n\t\t\t\tChart.render();\n\t\t\t\treturn Chart;\n\t\t\t}\n\t\t};\n\t\tChart.init();\n\n\t\treturn Chart;\n\t}\n\n\t// create charts\n\tconst charts = this.nodes().map(createChart);\n\treturn charts.length > 1 ? charts : charts.pop();\n};\n"},"lineCount":null}},"error":null,"hash":"9dcf8958c421fc7022cd8195c1c7bb5b","cacheData":{"env":{}}}